<%= form_for(@rank) do |f| %>
  <% if @rank.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@rank.errors.count, "error") %> prohibited this rank from being saved:</h2>

      <ul>
      <% @rank.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class ="field">
    <label for = "rank_food">Food : <%= @food %></label>
    <br>
    <input type = "hidden" name="rank[food]" id="rank_food" value = <%=@food%> >
  </div>
  <div class="field">
    <%= f.label :nickname %><br>
    <%= f.text_field :nickname %>
  </div>
  <div class="field">
    <%= f.label :content %><br>
    <%= f.text_area :content %>
  </div>
  <div class="field">
    <%= f.label :star %><br>
    <%= f.text_field :star %>
  </div>
 
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
