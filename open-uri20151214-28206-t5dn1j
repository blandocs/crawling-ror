<script language="JavaScript">
	function PrintMenu() {
		URL = "restaurant_menu_print.asp?gubun=1&date=2015-12-15";
		window.open (URL,"print", "width=620,height=700,scrollbars=no, left=10, top=10");
	}

	//하루동안 이창을 열지않음 소스입니다
	function GetCookie(name){
		var cname = name + "=";
		var dc = document.cookie;
		var val = "";
		if (dc.length > 0) {
			begin = dc.indexOf(cname);
			if (begin != -1) {
				begin += cname.length;
				end = dc.indexOf(";", begin);
				if (end == -1) end = dc.length;
				val += unescape(dc.substring(begin, end));

			}
		}
		return val;
	}

	/* 감사의달 이벤트 팝업 */
	/*
	if (GetCookie('cookieEvent0507') != "pop0507") {
		window.open('/popup/20100507/main_popup_100507.html', 'noticepop0507','scrollbars=no,width=602,height=920,resizable=no,left=80,top=10');
	}
	*/
</script>
