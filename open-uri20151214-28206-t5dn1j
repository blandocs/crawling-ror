<script language="javascript" type="text/javascript">

	function Map_Popup() {
		window.open('/html/map/map_01.asp?flag=1&flag2=1','map_pop','width=916,height=656');
	}


	function openURL(menu){

		if (menu=="SUNMALL"){
			frmMenu.location.href="/common/inc/main_meun_counter.asp?menu="+menu;
			location.href = "/html/snumall/l_goods_list2.asp?l_cd=1";
		}

		if (menu=="STORE"){
			frmMenu.location.href="/common/inc/main_meun_counter.asp?menu="+menu;
			location.href = "/html/store/store_main.asp";
		}


		if (menu=="REST"){
			frmMenu.location.href="/common/inc/main_meun_counter.asp?menu="+menu;
			location.href = "/html/restaurant/main.asp";
		}

		if (menu=="LIFE"){
			frmMenu.location.href="/common/inc/main_meun_counter.asp?menu="+menu;
			location.href = "/html/life/life_main.asp";
		}

		if (menu=="CAFE"){
			frmMenu.location.href="/common/inc/main_meun_counter.asp?menu="+menu;
			location.href = "/html/community/community_main.asp";
		}

		if (menu=="PARTNER"){
			frmMenu.location.href="/common/inc/main_meun_counter.asp?menu="+menu;
			location.href = "/html/partner/partner_main.asp";
		}


		if (menu=="MAP"){
			frmMenu.location.href="/common/inc/main_meun_counter.asp?menu="+menu;
			Map_Popup();
		}

	}

</script>

